{
  "manifest_version": 2,
  "name": "YT playback adjuster",
  "version": "1.0.0",
  "description": "Adjusts YouTube video timer based on playback speed",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ]
}
